
int main() 
{
    //Drive up to platforms
    LeftMotor.rotateFor(30,rotationUnits::deg,100,velocityUnits::pct);
    RightMotor.rotateFor(-30,rotationUnits::deg,100,velocityUnits::pct);
    LeftMotor.rotateFor(30,rotationUnits::rev,75,velocityUnits::pct);
    RightMotor.rotateFor(30,rotationUnits::rev,75,velocityUnits::pct);

}
